<div class="container">
  <h1>Listing Extratos</h1>
  <table class="table">
    <thead>
      <tr>
        <th>Valor</th>
        <th>Tipo de lançamento</th>
        <th>Data de lançamento</th>
      </tr>
    </thead>
  
    <tbody>
      <% @extratos.each do |extratos| %>
        <tr>
          <td><%= extratos.valor %></td>
          <td><%= extratos.tipo_lancamento %></td>
          <td><%= extratos.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>